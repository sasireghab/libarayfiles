import groovy.json.JsonSlurper

echo "Hello from jenkins shared library jenkinsfile....."
//def pipelineParams= [:]
def test = new JsonSlurper().parseText '''
{"resources": [
    {
        "metadata": "entity",
        "entity": "system"
    }
]}
'''

test.resources.each {
    println it.metadata
    println it.entity
}

myTemplate{ test }
